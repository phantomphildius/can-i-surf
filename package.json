{
  "private": true,
  "workspaces": ["client", "server"],
  "engines": {
    "node": ">= 14.0.0 <= 16.13.1",
    "yarn": "1.x"
  },
  "scripts": {
    "postinstall": "tsc -p server/",
    "deploy": "git add -A && git commit -m Heroku && git push heroku main",
    "start": "node server/dist/app.js",
    "heroku-postbuild": "cd client && yarn build"
  }
}
